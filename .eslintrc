{ 
   "extends": ["airbnb/base"],
   "env": {
        "browser": true,
        "node": true
    },
    "rules": {
        "no-param-reassign": ["error", { "props": false }],
        "linebreak-style": 0,
        "object-curly-newline":["error", { "ImportDeclaration": "always", "ExportDeclaration": "never" }]
    }
}